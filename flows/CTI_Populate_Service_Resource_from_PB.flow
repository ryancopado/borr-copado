        <defaultConnectorLabel>Secondary Number Not Found</defaultConnectorLabel>
        <rules>
            <name>Secondary_Match_Found1</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Secondary_Number</leftValueReference>
