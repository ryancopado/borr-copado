        </defaultConnector>
        <defaultConnectorLabel>Primary Number Not Found</defaultConnectorLabel>
        <rules>
            <name>Match_Found1</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Service_Resource_w_Customer_Number</leftValueReference>
