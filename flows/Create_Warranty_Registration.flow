    <formulas>
        <name>AccountId</name>
        <dataType>String</dataType>
        <expression>IF( NOT( ISBLANK( {!ScreenLookup_CustomerContactAccount.recordId} )) , 
{!ScreenLookup_CustomerContactAccount.recordId} , 
{!Create_Customer_Contact_Account}
)</expression>
    </formulas>
    <formulas>
        <name>Calculate_SiteEndDate</name>
        <dataType>Date</dataType>
        <expression>DATE(
YEAR( {!Calculate_SiteStartDate} ) + {!Screen_EPC_Defect_Warranty_Term_yrs} ,
MONTH( {!Calculate_SiteStartDate} ) ,
DAY ( {!Calculate_SiteStartDate} )
 )</expression>
    </formulas>
    <formulas>
        <name>Calculate_SiteStartDate</name>
        <dataType>Date</dataType>
        <expression>CASE( {!Screen_Warranty_Activation_Milestone} , 
&quot;Substantial Completion&quot; , {!Loop_Site_Records.Substantial_Completion_NEW__c},
&quot;Permission to Operate&quot; , {!Loop_Site_Records.PTO_Date__c} ,
&quot;Final Completion&quot; , {!Loop_Site_Records.Final_Completion__c} , 
&quot;Mechanical Completion&quot; , {!Loop_Site_Records.Substantial_Completion_Date__c},
Null )</expression>
    </formulas>
