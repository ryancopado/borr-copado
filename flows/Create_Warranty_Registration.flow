&amp;
IF(LEN({!O_M_Agreement_Comments_0}) &gt; 0 , &quot;O&amp;M Agreement: &quot; &amp; {!O_M_Agreement_Comments_0} &amp; BR(), &quot;&quot;)
&amp;
IF(LEN({!Civil_Contract_Comments_0}) &gt; 0 , &quot;Civil Contract: &quot; &amp; {!Civil_Contract_Comments_0} &amp; BR() , &quot;&quot;)
&amp;
IF(LEN({!Electrical_Contract_Comments_0}) &gt; 0 , &quot;Electrical Contract: &quot; &amp; {!Electrical_Contract_Comments_0} &amp; BR() , &quot;&quot;)
&amp;
IF(LEN({!Mechanical_Contract_Comments}) &gt; 0 , &quot;Mechanical Contract: &quot; &amp; {!Mechanical_Contract_Comments} &amp; BR() , &quot;&quot;)
&amp;
IF(LEN({!Record_Drawing_Comments_0}) &gt; 0 , &quot;Record Drawings: &quot; &amp; {!Record_Drawing_Comments_0} &amp; BR() , &quot;&quot;)</expression>
