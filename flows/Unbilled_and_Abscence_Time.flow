    <decisions>
        <name>Does_Time_Sheet_Exist</name>
        <label>Does Time Sheet Exist?</label>
        <locationX>446</locationX>
        <locationY>101</locationY>
        <defaultConnector>
            <targetReference>Is_Time_Sheet_Approved</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Does_Not_Exist</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>TimeSheetId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <label>Does Not Exist</label>
        </rules>
    </decisions>
    <decisions>
        <description>If the Admin time type is unavailable, then do not create a time sheet entry. This is used so that the FSL scheduler optimizer won&apos;t create TSE&apos;s in which accounting will use for billing.</description>
        <name>Is_Admin_Time_Unavailable</name>
        <label>Is Admin Time Type &quot;Unavailable&quot; ?</label>
        <locationX>928</locationX>
        <locationY>111</locationY>
        <defaultConnector>
            <targetReference>Create_Time_Sheet_Entry</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Type_is_Unavailable</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Type</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Unavailable</stringValue>
                </rightValue>
            </conditions>
            <label>Type is Unavailable</label>
        </rules>
    </decisions>
    <decisions>
        <description>If time sheet is approved, cancel process. we do not want TSE&apos;s for already approved TST&apos;s</description>
        <name>Is_Time_Sheet_Approved</name>
        <label>Is Time Sheet Approved?</label>
        <locationX>724</locationX>
        <locationY>104</locationY>
        <defaultConnector>
            <targetReference>Is_Admin_Time_Unavailable</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>TSTApproved</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>TSTStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Approved</stringValue>
                </rightValue>
            </conditions>
            <label>TSTApproved</label>
        </rules>
    </decisions>
    <description>Created decision based on Admin Time Type</description>
