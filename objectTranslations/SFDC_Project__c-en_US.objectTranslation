        <errorMessage><!-- If Not To Exceed is non-zero, it can not be less than Total Invoiced. --></errorMessage>
        <name>NTE_Not_Less_Than_Total_Invoiced</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Only David Marks, Matthew Murphy, or Gary Buchanan can change the Commissioning Status to &quot;Approved&quot; --></errorMessage>
        <name>Only_David_or_Gary_can_change_to_Approve</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Only David Marks, Matthew Murphy, or Gary Buchanan can change the Commissioning Status to &quot;First Stage Approved&quot; --></errorMessage>
        <name>Only_David_or_Gary_can_change_to_FirstAp</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Status cannot be changed until the Contract is Signed. --></errorMessage>
        <name>PreContract</name>
    </validationRules>
