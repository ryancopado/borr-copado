        <errorMessage><!-- Commissioning Status must be Approved to complete the project --></errorMessage>
        <name>Commissioning_Approved_to_Complete</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Not To Exceed must be greater than zero for Fixed Cost project.  Should be set to maximum cost of project to the customer. --></errorMessage>
