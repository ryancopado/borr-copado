    <validationRules>
        <fullName>AcctSeedERP__Location_Is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(AcctSeedERP__Location__c)</errorConditionFormula>
        <errorDisplayField>AcctSeedERP__Location__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AcctSeedERP__Outbound_Less_Than_Equal_Inbound</fullName>
        <active>true</active>
        <description>Ensure outbound inventory movements are less than or equal to outbound inventory movements</description>
        <errorConditionFormula>(AcctSeedERP__Manufacturing_Quantity__c +  AcctSeedERP__Purchasing_Quantity__c +  AcctSeedERP__Inbound_Quantity__c) &lt; ( AcctSeedERP__Sales_Order_Quantity__c +  AcctSeedERP__Outbound_Quantity__c)</errorConditionFormula>
        <errorMessage>The aggregate outbound quantity on the inventory balance cannot be more than the aggregate inbound quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AcctSeedERP__Prevent_Update_Location_if_Movements</fullName>
        <active>true</active>
        <description>Prevent changing the location if there are associated inventory movements</description>
        <errorConditionFormula>NOT(ISNEW()) &amp;&amp; (AcctSeedERP__Inbound_Quantity__c &gt; 0 ||  AcctSeedERP__Outbound_Quantity__c &gt; 0 ||  AcctSeedERP__Manufacturing_Quantity__c &gt; 0 ||  AcctSeedERP__Sales_Order_Quantity__c &gt; 0 ||  AcctSeedERP__Purchasing_Quantity__c &gt; 0) &amp;&amp;  PRIORVALUE(AcctSeedERP__Location__c) != AcctSeedERP__Location__c</errorConditionFormula>
        <errorDisplayField>AcctSeedERP__Location__c</errorDisplayField>
        <errorMessage>You cannot change the location if there are associated inventory movements. You must delete all inventory movements in order to change the location.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AcctSeedERP__Prevent_Update_Product_if_Movements</fullName>
        <active>true</active>
        <description>Prevent changing the product if there are associated inventory movements</description>
        <errorConditionFormula>NOT(ISNEW()) &amp;&amp; (AcctSeedERP__Inbound_Quantity__c &gt; 0 ||  AcctSeedERP__Outbound_Quantity__c &gt; 0 ||  AcctSeedERP__Manufacturing_Quantity__c &gt; 0 ||  AcctSeedERP__Sales_Order_Quantity__c &gt; 0 ||  AcctSeedERP__Purchasing_Quantity__c &gt; 0) &amp;&amp;  PRIORVALUE(AcctSeedERP__Product__c) != AcctSeedERP__Product__c</errorConditionFormula>
        <errorDisplayField>AcctSeedERP__Product__c</errorDisplayField>
        <errorMessage>You cannot change the product if there are associated inventory movements. You must delete all inventory movements in order to change the product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AcctSeedERP__Prevent_Update_Warehouse_if_Movements</fullName>
        <active>true</active>
        <description>Prevent changing the warehouse if there are associated inventory movements</description>
        <errorConditionFormula>NOT(ISNEW()) &amp;&amp; (AcctSeedERP__Inbound_Quantity__c &gt; 0 ||  AcctSeedERP__Outbound_Quantity__c &gt; 0 ||  AcctSeedERP__Manufacturing_Quantity__c &gt; 0 ||  AcctSeedERP__Sales_Order_Quantity__c &gt; 0 ||  AcctSeedERP__Purchasing_Quantity__c &gt; 0) &amp;&amp;  PRIORVALUE(AcctSeedERP__Warehouse__c) != AcctSeedERP__Warehouse__c</errorConditionFormula>
        <errorDisplayField>AcctSeedERP__Warehouse__c</errorDisplayField>
        <errorMessage>You cannot change the warehouse if there are associated inventory movements. You must delete all inventory movements in order to change the warehouse.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AcctSeedERP__Product_Is_Inventory_Product</fullName>
        <active>true</active>
        <description>Only inventoried products can be associated with an inventory balance</description>
        <errorConditionFormula>AcctSeedERP__Product__r.AcctSeed__Inventory_Product__c = false</errorConditionFormula>
        <errorDisplayField>AcctSeedERP__Product__c</errorDisplayField>
