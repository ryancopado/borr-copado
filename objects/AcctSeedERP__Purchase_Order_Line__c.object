        <fullName>Flag__c</fullName>
        <description>indicates value based on various status of PO, POL and when the record was created.</description>
        <externalId>false</externalId>
        <formula>IF(
AND(
ISPICKVAL( AcctSeedERP__Purchase_Order__r.AcctSeedERP__Status__c, &quot;Open&quot;),  
ISPICKVAL( PO_Line_Status__c , &quot;Open&quot;),  
DATEVALUE(CreatedDate + 2)   &lt;=   TODAY()
),&quot;Not Yet Submitted&quot;,

IF(
AND(
OR(ISPICKVAL( AcctSeedERP__Purchase_Order__r.AcctSeedERP__Status__c, &quot;PM Approved&quot;), ISPICKVAL( AcctSeedERP__Purchase_Order__r.AcctSeedERP__Status__c, &quot;Aaron Approved&quot;)),
ISPICKVAL( PO_Line_Status__c , &quot;Open&quot;)),  &quot;Not Yet Submitted&quot;,

IF(
AND(
ISPICKVAL( AcctSeedERP__Purchase_Order__r.AcctSeedERP__Status__c, &quot;Need More Information&quot;),  
ISPICKVAL( PO_Line_Status__c , &quot;Open&quot;)), &quot;Missing Information&quot;,

IF(
AND(
OR(
ISPICKVAL( AcctSeedERP__Purchase_Order__r.AcctSeedERP__Status__c, &quot;Invoice Received&quot;),  ISPICKVAL( AcctSeedERP__Purchase_Order__r.AcctSeedERP__Status__c, &quot;Invoice Processed&quot;)),
POLine_Total__c -  Roll_Up_All_Account_Payable_Lines__c = 0,
TODAY() &gt;  (AcctSeedERP__Purchase_Order__r.PO_Status_Change_Date__c +7)), &quot;Invoice Not Processed&quot;,


IF(
AND(
OR(
ISPICKVAL(  PO_Line_Status__c , &quot;Vendor Shipped Material&quot;),  ISPICKVAL(  PO_Line_Status__c , &quot;Shipment Scheduled&quot;), ISPICKVAL(  PO_Line_Status__c , &quot;Partially Received: Warehouse&quot;), ISPICKVAL(  PO_Line_Status__c , &quot;Partially Received: Job Site&quot;), ISPICKVAL(  PO_Line_Status__c , &quot;Received In-Full: Warehouse&quot;), ISPICKVAL(  PO_Line_Status__c , &quot;Received In-Full: Job Site&quot;)),
POLine_Total__c -  Roll_Up_All_Account_Payable_Lines__c  &lt;&gt;  0,
TODAY() &gt;  ( POLStatusChangeDate__c  +7)), &quot;Invoice Missing&quot;,

IF( 
OR(
AND(
ISPICKVAL( PO_Line_Status__c , &quot;Vendor Received PO&quot;),
Vendor_Confirmed_Ship_Date__c &lt; TODAY()),
AND(
ISPICKVAL( PO_Line_Status__c , &quot;Submitted to Vendor&quot;),
Vendor_Confirmed_Ship_Date__c &lt; TODAY())), &quot;Confirm In Transit/Received&quot;,

IF(
AND(
ISPICKVAL( AcctSeedERP__Purchase_Order__r.AcctSeedERP__Status__c, &quot;Vendor Shipped Material&quot;),
ISPICKVAL( PO_Line_Status__c , &quot;Vendor Shipped Material&quot;),
ISBLANK(Vendor_Confirmed_Ship_Date__c )
), &quot;Ship Date Not Populated&quot;,

IF( 
AND(
OR(ISPICKVAL( PO_Line_Status__c , &quot;Vendor Received PO&quot;),ISPICKVAL( PO_Line_Status__c , &quot;Submitted to Vendor&quot;)),
Vendor_Confirmed_Ship_Date__c &gt; TODAY()), &quot;Pending Shipment Approaching&quot;,

IF(
AND(
OR(ISPICKVAL( PO_Line_Status__c , &quot;Vendor Received PO&quot;), ISPICKVAL( PO_Line_Status__c , &quot;Submitted to Vendor&quot;)),
ISBLANK( AcctSeedERP__Purchase_Order__r.SOA_Returned__c ),
NOT(ISBLANK( Vendor_Confirmed_Ship_Date__c)),
AcctSeedERP__Purchase_Order__r.AcctSeedERP__Order_Date__c  &lt; (TODAY()-7)
), &quot;SOA Missing&quot;,

IF(
AND(
OR(
ISPICKVAL( PO_Line_Status__c , &quot;Vendor Received PO&quot;), ISPICKVAL( PO_Line_Status__c , &quot;Material Deposit Paid&quot;)
),
ISBLANK(Vendor_Confirmed_Ship_Date__c )
), &quot;Confirm Ship Date&quot;,


IF(
ISPICKVAL( PO_Line_Status__c , &quot;Closed&quot;), &quot;Closed&quot;,

IF(
ISPICKVAL( PO_Line_Status__c , &quot;Rejected&quot;), &quot;Rejected&quot;,
IF(
ISPICKVAL( PO_Line_Status__c , &quot;Cancelled&quot;), &quot;Cancelled&quot;,



IF(
ISPICKVAL( PO_Line_Status__c , &quot;Shipment On Hold&quot;), &quot;On Hold&quot;,


IF(
POLine_Total__c -  Roll_Up_All_Account_Payable_Lines__c = 0, &quot;All Costs Received&quot;,

IF(
POLine_Total__c &gt; Roll_Up_All_Account_Payable_Lines__c, &quot;Confirm In Transit/Received&quot;,

NULL
))))))))))))))))</formula>
