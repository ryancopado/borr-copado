    <validationRules>
        <fullName>AcctSeedERP__Product_Inventory_Type</fullName>
        <active>true</active>
        <description>The product field must reference a product with an inventory type of purchased or raw material</description>
        <errorConditionFormula>(TEXT(AcctSeedERP__Product__r.AcctSeed__Inventory_Type__c) != &quot;Purchased&quot; &amp;&amp; TEXT(AcctSeedERP__Product__r.AcctSeed__Inventory_Type__c) != &quot;Raw Material&quot;) ||
(TEXT(AcctSeedERP__Product__r.AcctSeed__Inventory_Type__c) = &quot;Raw Material&quot; &amp;&amp; AcctSeedERP__Product__r.AcctSeed__Inventory_Product__c = false)</errorConditionFormula>
        <errorDisplayField>AcctSeedERP__Product__c</errorDisplayField>
        <errorMessage>The product inventory type must be purchased or raw material. If the product inventory type is raw material the inventory product field must be checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AcctSeedERP__Product_Is_Required</fullName>
        <active>true</active>
        <description>product is required</description>
        <errorConditionFormula>ISBLANK(AcctSeedERP__Product__c)</errorConditionFormula>
        <errorDisplayField>AcctSeedERP__Product__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AcctSeedERP__Project_Task_Required_With_Project</fullName>
        <active>true</active>
        <description>Rule ensures a Project Task is Required if the Project field is used.</description>
        <errorConditionFormula>NOT(ISBLANK(AcctSeedERP__Project__c)) &amp;&amp; ISBLANK(AcctSeedERP__Project_Task__c)</errorConditionFormula>
        <errorDisplayField>AcctSeedERP__Project_Task__c</errorDisplayField>
        <errorMessage>A Project Task is required if a Project is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AcctSeedERP__Quantity_Is_Positive</fullName>
        <active>false</active>
        <description>quantity must be greater than or equal to 0</description>
        <errorConditionFormula>AcctSeedERP__Quantity__c &lt; 0</errorConditionFormula>
        <errorDisplayField>AcctSeedERP__Quantity__c</errorDisplayField>
        <errorMessage>quantity must be greater than or equal to 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_PM_UnitCost</fullName>
        <active>true</active>
        <description>If user is a Project Manager do not allow the Unit Cost field to increase and only allow the the Unit Cost field to decrease if the associated PO is Not To Exceed.</description>
        <errorConditionFormula>( $Profile.Name = &quot;Borrego - PM&quot;) &amp;&amp;
( (PRIORVALUE( Unit_Cost__c ) &lt; Unit_Cost__c ) ||
( (PRIORVALUE( Unit_Cost__c ) &gt; Unit_Cost__c ) &amp;&amp;
(NOT( ISPICKVAL( AcctSeedERP__Purchase_Order__r.Not_To_Exceed__c , &quot;Yes&quot;) ) ) ) )</errorConditionFormula>
        <errorDisplayField>Unit_Cost__c</errorDisplayField>
        <errorMessage>To decrease Unit Cost, the associated PO must be Not To Exceed. Cannot increase Unit Cost. Instead, enter a Change Order.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_UnitPriceandQuantity</fullName>
        <active>true</active>
        <description>Unless the user is a System Administrator or has Accounting in their profile name, do not allow the Quantity or Unit Price fields to change.</description>
        <errorConditionFormula>( ISCHANGED( AcctSeedERP__Quantity__c ) || ISCHANGED( AcctSeedERP__Unit_Price__c ) ) &amp;&amp; NOT( $Profile.ManageUsers || CONTAINS( $Profile.Name , &quot;Accounting&quot;) || CONTAINS( $Profile.Name , &quot;Procurement&quot;) )</errorConditionFormula>
        <errorMessage>Only System Administrators and Accounting users can change Quantity or Unit Price.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Fields_From_Being_Edited_by_a_PM</fullName>
        <active>true</active>
        <description>Prevents users with the PM Profile from editing certain fields</description>
        <errorConditionFormula>AND($Profile.Name = &quot;Borrego - PM&quot;, 

OR(
ISCHANGED( AcctSeedERP__Product__c ), 
ISCHANGED( AcctSeedERP__Quantity__c ), 
ISCHANGED( Requested_Delivery_Date__c ), 
ISCHANGED( Vendor_Confirmed_Ship_Date__c ), 
ISCHANGED( AcctSeedERP__Unit_Price__c ), 
ISCHANGED( Freight__c ), 
ISCHANGED( Sales_Tax_Rate_Percent__c ), 
ISCHANGED( PO_Line_Status__c ) 
) 
)</errorConditionFormula>
        <errorMessage>Certain fields cannot be edited. Please contact Accounting if you need to change the Status or the Vendor and Shipping Information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_Greater_than_0</fullName>
        <active>false</active>
        <errorConditionFormula>Quantity_greater_than_0__c =&apos;No&apos;</errorConditionFormula>
        <errorDisplayField>AcctSeedERP__Quantity__c</errorDisplayField>
        <errorMessage>The Quantity must be greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Update_Site_when_Project_changed</fullName>
        <active>false</active>
        <errorConditionFormula>AND  ( 
ISCHANGED( AcctSeedERP__Project__c ), 
NOT( ISCHANGED ( Site__c ) ) )</errorConditionFormula>
        <errorDisplayField>Site__c</errorDisplayField>
        <errorMessage>Site must be updated when changing the Project</errorMessage>
    </validationRules>
