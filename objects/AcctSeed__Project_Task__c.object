    <validationRules>
        <fullName>AcctSeed__Value_for_Labor_GL_Account</fullName>
        <active>false</active>
        <description>Verifies labor gl account field is not blank.</description>
        <errorConditionFormula>ISBLANK( AcctSeed__Labor_GL_Account__c )</errorConditionFormula>
        <errorMessage>A project task must contain a value for the labor gl account field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AcctSeed__Value_for_Project</fullName>
        <active>true</active>
        <description>Verifies project field is not blank.</description>
        <errorConditionFormula>ISBLANK( AcctSeed__Project__c )</errorConditionFormula>
        <errorMessage>A project task must contain a value for the project field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Active_budget_read_only</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
RecordTypeId = &quot;012800000007gu8&quot;, 
OR( Name = &quot;OH/Indirect Costs&quot;, Name = &quot;Internal Engineering&quot;),
ISCHANGED( Active_Budget__c ),
$User.Department &lt;&gt; &quot;Accounting&quot;, 
$User.Id &lt;&gt; &quot;00530000000rc0K&quot;,
AcctSeed__Project__r.PM_Lock__c = TRUE, 
NOW() &gt; AcctSeed__Project__r.PM_Lock_timestamp__c )</errorConditionFormula>
        <errorMessage>You may not modify the Active Budget for OH/Indirects until the budget is locked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Line_Items_Edited_Only_by_Acctin</fullName>
        <active>false</active>
        <description>prevents users outside of accounting from editing fields specific to billing line items.</description>
        <errorConditionFormula>AND(RecordType.Id = &quot;012800000007gu7&quot;, 
$User.Department &lt;&gt; &quot;Accounting&quot;, 
ISCHANGED(AcctSeed__Budgeted_Revenue__c) || ISCHANGED( Notes__c ) || ISCHANGED( Name ) || ISCHANGED( AcctSeed__GL_Account_Variable_1__c )) || ISCHANGED ( AcctSeed__Sort_Order__c)  || ISCHANGED( AcctSeed__Project__c ) || ISCHANGED ( Roll_Up_Transactions__c )</errorConditionFormula>
        <errorMessage>Only Accounting can edit Billing Line Items</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Budget_Locked</fullName>
        <active>false</active>
        <errorConditionFormula>AND (ISCHANGED( PM_Budget__c),AcctSeed__Project__r.PM_Lock__c =true)</errorConditionFormula>
        <errorMessage>You cannot add/edit a PM Budget value once the PM budget is locked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Budgeted_Revenue_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 $User.Alias &lt;&gt; &quot;rdani&quot;,
NOT(CONTAINS(TEXT($User.Ops_Division__c) , &quot;Dev&quot; )),
RecordTypeId = &quot;012800000007gu7&quot;,
ISBLANK( AcctSeed__Budgeted_Revenue__c ))</errorConditionFormula>
        <errorDisplayField>AcctSeed__Budgeted_Revenue__c</errorDisplayField>
        <errorMessage>Budgeted Revenue is required for Billing Line Items</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Line_Item_Cost_Code_read_only</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT( ISNEW () ), 
NOT( OR( $User.Id = &apos;00530000000rc0K&apos;, $User.ProfileId = &quot;00e30000000iBpb&quot;, CONTAINS($User.Department,&quot;Accounting&quot;) ) ), 
OR( ISCHANGED( Name ), ISCHANGED( Cost_Code__c ) ), 
RecordTypeId = &quot;012800000007gu8&quot;)</errorConditionFormula>
        <errorMessage>You cannot change the Cost Code or Line Item name. Please contact Accounting or Jason Askins to do this.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Line_Item_and_Cost_Code_Match</fullName>
        <active>false</active>
        <description>The lne item name and the cost code name must match</description>
        <errorConditionFormula>AND(RecordTypeId = &quot;012800000007gu8&quot;,Name &lt;&gt; Cost_Code__r.Name, 
OR(Name &lt;&gt; &quot;Retainage&quot;, Name &lt;&gt; &quot;Retention&quot;))</errorConditionFormula>
        <errorMessage>The line item name should be the same as the cost code name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OH_Ind_Int_Eng_only_edit_by_Accting</fullName>
        <active>false</active>
        <description>Prevents anyone who is not in Accounting or Jason Askins from modifying Overhead/Indirects &amp; Internal Engineering budget line items.</description>
        <errorConditionFormula>AND( 
RecordTypeId = &quot;012800000007gu8&quot;, 
OR( Name = &quot;OH/Indirect Costs&quot;, Name = &quot;Internal Engineering&quot;), 
OR( ISCHANGED( Sales_Budget__c ), 
ISCHANGED( PM_Budget__c ), 
ISCHANGED( Active_Budget__c ) ), 
$User.Department &lt;&gt; &quot;Accounting&quot;, 
$User.Id &lt;&gt; &quot;00530000000rc0K&quot;, 
AND( 
AcctSeed__Project__r.PM_Lock__c = TRUE, 
PRIORVALUE( Active_Budget__c ) &lt;&gt; 0 ) )</errorConditionFormula>
        <errorMessage>Overhead/Indirects &amp; Internal Engineering Budget Line Items. can only be modified by Accounting &amp; Jason Askins.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Costs_Other</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW(), 
Cost_Code__r.Id = &quot;a0H8000000I6Hub&quot;, 
NOT( Name = &quot;Expense Reports&quot; ), 
NOT( Name = &quot;Expense Report&quot; ), 
NOT( Name = &quot;Other Costs- Other&quot; ) )</errorConditionFormula>
        <errorMessage>When using Other Costs- Other cost code, the Line Item name must be either &quot;Expense Report&quot; or &quot;Other Costs- Other&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Budget_Locked</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  AcctSeed__Project__r.Sales_Budget_Locked__c,
  OR(
    AND(
      ISNEW(),
      NOT(ISBLANK(Sales_Budget__c))
    ),
    ISCHANGED(Sales_Budget__c)
  )
)</errorConditionFormula>
        <errorMessage>You cannot add/edit a Sales Budget value once the Sales Budget is locked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Budget_read_only</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
RecordTypeId = &quot;012800000007gu8&quot;, 
OR( Name = &quot;OH/Indirect Costs&quot;, Name = &quot;Internal Engineering&quot; ),
$User.Department &lt;&gt; &quot;Accounting&quot;, 
$User.Id &lt;&gt; &quot;00530000000rc0K&quot;,
$User.Title &lt;&gt; &quot;Applications Engineer&quot;,
AND( TEXT( PRIORVALUE( Sales_Budget__c ) ) &lt;&gt; NULL, PRIORVALUE( Sales_Budget__c ) &lt;&gt; 0 ), 
OR( ISCHANGED( Sales_Budget__c ), ISCHANGED( PM_Budget__c ) ) )</errorConditionFormula>
        <errorMessage>Please contact Accounting or Jason Askins to make changes to Sales, PM or Active Budgets for Internal Engineering and Overhead Line Items</errorMessage>
    </validationRules>
