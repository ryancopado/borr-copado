    <validationRules>
        <fullName>AcctSeed__ExternalIDNotEditable</fullName>
        <active>true</active>
        <description>The External ID field can only be set at the time of creation. It cannot be updated or modified.</description>
        <errorConditionFormula>AND ( 
NOT(ISNEW()),
ISCHANGED(AcctSeed__External_Id2__c)
)</errorConditionFormula>
        <errorDisplayField>AcctSeed__External_Id2__c</errorDisplayField>
        <errorMessage>The External ID field can only be set at the time of creation. It cannot be updated or modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AcctSeed__ReverseChargeNotAllowedIfExternalIDBlank</fullName>
        <active>true</active>
        <description>Reverse-Charge is not allowed to be checked if the External ID field is blank.</description>
        <errorConditionFormula>AND(
ISBLANK(AcctSeed__External_Id2__c),
AcctSeed__Reverse_Charge__c = true)</errorConditionFormula>
        <errorDisplayField>AcctSeed__Reverse_Charge__c</errorDisplayField>
        <errorMessage>Reverse-Charge is not allowed to be checked if the External ID field is blank.</errorMessage>
    </validationRules>
