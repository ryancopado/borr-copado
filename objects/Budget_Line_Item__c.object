    <validationRules>
        <fullName>Budget_Locked</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Budget__r.PM_Lock__c = true, OR(PRIORVALUE(PM_Initial_Projection__c) &lt;&gt; PM_Initial_Projection__c, AND(ISNEW() = true, PM_Initial_Projection__c &gt; 0)))</errorConditionFormula>
        <errorDisplayField>PM_Initial_Projection__c</errorDisplayField>
        <errorMessage>You cannot add/edit an Initial Projection value once the PM budget is locked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Budget_Unlocked</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Budget__r.PM_Lock__c = false, AND(ISNEW() = true,  OR(Active_Budget__c  &gt; 0, Actual_Budget__c  &gt; 0)))</errorConditionFormula>
        <errorMessage>You cannot add/edit the Active Budget or Actual Budget value before the budget is locked.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
