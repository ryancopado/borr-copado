    <fields>
        <fullName>Amount__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL( Opportunity__r.O_M_Payment__c ,&apos;Upfront&apos;), (Percent__c *   Opportunity__r.Total_Project_Contract_Value__c),(Percent__c * Opportunity__r.Final_Contract_Price__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Total amount of the commission: Contract Price * Commission Percent</inlineHelpText>
        <label>Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Automated__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>If checked, Commission was created when contract signed button was pushed. Used only for Commission type records</inlineHelpText>
        <label>Automated</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Contact__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <inlineHelpText>Affiliate Contact receiving commission on the contract</inlineHelpText>
        <label>Contact</label>
        <referenceTo>Contact</referenceTo>
        <relationshipLabel>Commissions</relationshipLabel>
        <relationshipName>Commissions</relationshipName>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Earned_Commission__c</fullName>
        <externalId>false</externalId>
        <formula>Opportunity__r.Total_Project_Contract_Value__c  * Opportunity__r.Percent_Complete__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Earned Commission</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Fixed_Cost_Amount__c</fullName>
        <description>Commission&apos;s Fixed Cost Amount (manually entered)</description>
        <externalId>false</externalId>
        <inlineHelpText>Please enter the Commission&apos;s Fixed Cost Amount in this field</inlineHelpText>
        <label>Fixed Cost Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Fixed_Cost__c</fullName>
        <defaultValue>false</defaultValue>
        <description>For indicating a Fixed-Cost Commission.
Please add the Amount manually in the &apos;Fixed Cost Amount&apos; field.</description>
        <externalId>false</externalId>
        <inlineHelpText>For indicating a Fixed-Cost Commission.
Please add the Amount manually in the &apos;Fixed Cost Amount&apos; field.</inlineHelpText>
        <label>Fixed Cost</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Number_Percent__c</fullName>
        <externalId>false</externalId>
        <formula>Percent__c  * 100</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Number Percent</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Opportunity1__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Opportunity1</label>
        <referenceTo>Opportunity</referenceTo>
        <relationshipLabel>Commissions (Opportunity)</relationshipLabel>
        <relationshipName>Commissions1</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
