    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Adding_Self</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Automated__c = false, User__c = $User.Id)</errorConditionFormula>
        <errorDisplayField>User__c</errorDisplayField>
        <errorMessage>You cannot create a commission for which you are the user.</errorMessage>
    </validationRules>
