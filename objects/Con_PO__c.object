    <validationRules>
        <fullName>ARRA_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordTypeId =&quot;012800000006dRw&quot;, ISBLANK(TEXT( ARRA_Compliant__c)),   ISPICKVAL(Status__c, &quot;PM Approved&quot;))</errorConditionFormula>
        <errorDisplayField>ARRA_Compliant__c</errorDisplayField>
        <errorMessage>Please enter if the component needs to be ARRA compliant</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Aaron_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND( $User.FirstName &lt;&gt; &quot;Aaron&quot;, ISPICKVAL(Status__c, &quot;Approved by Aaron&quot;))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Only Aaron Hall can change the Status to Approved by Aaron.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Anita_Lynne_Mary_only_change_vendor_date</fullName>
        <active>true</active>
        <description>Only Lynne, Sara, and Anita can change the vendor delivery date</description>
        <errorConditionFormula>AND( (ISCHANGED( Expected_Delivery_Date__c )), $User.LastName &lt;&gt; &quot;Parker&quot;, $User.LastName &lt;&gt; &quot;Frieler&quot;, $User.LastName &lt;&gt; &quot;Orozco&quot; )</errorConditionFormula>
        <errorDisplayField>Expected_Delivery_Date__c</errorDisplayField>
        <errorMessage>Only Lynne, Mary, or Anita can update the Vendor Confirmed ship date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dollar_Cap</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW() = FALSE, $User.Id &lt;&gt; &apos;005300000012qpF&apos;, $User.FirstName &lt;&gt;&apos;Emily&apos;, $User.Id &lt;&gt; &apos;00580000003xepO&apos;,$User.Id &lt;&gt; &apos;00530000000jcLS&apos;, $User.Id &lt;&gt; &apos;00580000001qecY&apos;, Total_Cost_Tax__c - PRIORVALUE(Total_Cost_Tax__c) &gt; 200)</errorConditionFormula>
        <errorDisplayField>Accounting_Rejection_Notes__c</errorDisplayField>
        <errorMessage>Changes in total cost cannot exceed $200. 
If the change is greater than $200 you will need to recall the requisition and start again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Force_Rejection_Notes</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Status__c, &apos;Rejected&apos;),  Accounting_Rejection_Notes__c = &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Accounting_Rejection_Notes__c</errorDisplayField>
        <errorMessage>Please enter Rejection Notes so the PM will know what to fix.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW() = FALSE, ISPICKVAL(Status__c , &quot;PM Approved&quot;),  Cost_Code_Cat__c = &quot;02-1215 M&quot;,  Product__c =&apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Product__c</errorDisplayField>
        <errorMessage>Please enter the racking product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tax_Exemption</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW() = FALSE, Tax_Exemption__c &lt;&gt; &apos;Exempt&apos; , Tax_Exemption__c &lt;&gt; &apos;Non-Exempt&apos; , Tax_Exemption__c &lt;&gt; &apos;Not Entered&apos;, Tax_Exemption__c &lt;&gt; &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Tax_Exemption__c</errorDisplayField>
        <errorMessage>You may only enter Exempt or Non-Exempt as the Tax Exemption Status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Third_Party_Shipping</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW() = FALSE, ISPICKVAL(Third_Party_Shipping__c , &quot;Yes&quot;), Freight_Company__c = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Freight_Company__c</errorDisplayField>
        <errorMessage>You must enter a Freight Company when Third Party Shipping equals Yes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Vendor_Contact</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW() = FALSE, ISPICKVAL(Status__c , &quot;PM Approved&quot;),  Vendor_Contact__r.Email = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Vendor_Contact__c</errorDisplayField>
        <errorMessage>Vendor Contact Email is required. Please go the the Vendor Contact page and enter the email address.</errorMessage>
    </validationRules>
