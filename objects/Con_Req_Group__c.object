    <fields>
        <fullName>OK_to_Submit__c</fullName>
        <externalId>false</externalId>
        <formula>IF( AND(Vendor__r.AcctSeed__Accounting_Active__c =TRUE,  Number_of_Negative_Remaining_Budgets__c=0, BEGINS(Cost_Code_Number__c, &quot;0&quot;),  IsAttachment__c &lt;&gt;FALSE), IMAGE(&quot;/servlet/servlet.FileDownload?file=015800000015Isw&quot;, &quot;WHITE&quot;),

IF( AND(IsAttachment__c &lt;&gt;FALSE, OR(BEGINS(Cost_Code_Number__c, &quot;07&quot;), CONTAINS( Category__c , &quot;OH&quot;))) ,IMAGE(&quot;/servlet/servlet.FileDownload?file=015800000015Isw&quot;, &quot;WHITE&quot;),

IF(ISPICKVAL(Status__c,&quot;Approved&quot;), IMAGE(&quot;/servlet/servlet.FileDownload?file=015800000015IuY&quot;, &quot;WHITE&quot;), 

IF(Vendor__r.AcctSeed__Accounting_Active__c =FALSE,IMAGE(&quot;/servlet/servlet.FileDownload?file=015800000016gx0&quot;, &quot;WHITE&quot;),

IF(IsAttachment__c =FALSE,IMAGE(&quot;/servlet/servlet.FileDownload?file=015800000015sR0&quot;, &quot;WHITE&quot;), 

IF(AND( Number_of_Negative_Remaining_Budgets__c &gt;0, OR(NOT(BEGINS(Cost_Code_Number__c, &quot;07&quot;)), NOT(CONTAINS( Category__c , &quot;OH&quot;)))),IMAGE(&quot;/servlet/servlet.FileDownload?file=015800000015ItV&quot;, &quot;WHITE&quot;), 

&quot;NO&quot;))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>OK to Submit</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
