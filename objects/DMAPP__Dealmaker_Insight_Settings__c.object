        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>This setting determines whether the smart map browser and builder pages land on a multi publisher (namespace) view or an aggregated view.</description>
        <externalId>false</externalId>
        <inlineHelpText>This setting determines whether the smart map browser and builder pages land on a multi publisher (namespace) view or an aggregated view.</inlineHelpText>
        <label>Multi Publisher Front Page</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DMAPP__Number_of_days_of_logging_info__c</fullName>
        <defaultValue>90</defaultValue>
        <deprecated>false</deprecated>
        <description>This setting defines the interval, in days, after which search log data is deleted from the system.</description>
        <externalId>false</externalId>
        <inlineHelpText>This setting defines the interval, in days, after which search log data is deleted from the system.</inlineHelpText>
        <label>Number of days of logging info</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>DMAPP__Smart_Sales_Kit__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>This field is deprecated. Access to the Smart Library in Dealmaker Align is controlled by license allocation.</description>
        <externalId>false</externalId>
        <inlineHelpText>This field is deprecated. Access to the Smart Library in Dealmaker Align is controlled by license allocation.</inlineHelpText>
        <label>Smart Sales Kit (Deprecated)</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DMAPP__Solution_Segmentation__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Select this checkbox to make it possible for solutions to be organized into types. Solution search results will have a separate tab for each solution type.</description>
        <externalId>false</externalId>
        <label>Solution Types</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
