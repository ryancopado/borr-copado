<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>These settings control certain aspects of Dealmaker Align behavior.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>DMAPP__Align_Admin__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Enables deletion of any smart element in any namespace, both on the smart element list page and via Apex. Should only be enabled for a specific user. Do not enable this setting for the org.</description>
        <externalId>false</externalId>
        <inlineHelpText>Enables deletion of any smart element in any namespace, both on the smart element list page and via Apex. Should only be enabled for a specific user. Do not enable this setting for the org.</inlineHelpText>
        <label>Delete smart element from any namespace</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DMAPP__Default_Namespace__c</fullName>
        <deprecated>false</deprecated>
        <description>This is the default namespace that will be used on Insight objects that do not have a namespace defined.</description>
        <externalId>false</externalId>
        <inlineHelpText>This is the default namespace that will be used on Insight objects that do not have a namespace defined.</inlineHelpText>
        <label>Default Namespace</label>
        <length>80</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>DMAPP__Disable_Chatter__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>This setting disables chatter for Smart Maps</description>
        <externalId>false</externalId>
        <inlineHelpText>This setting disables chatter for Smart Maps</inlineHelpText>
        <label>Disable Chatter</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DMAPP__Disable_Solutions_Count__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Disable the solution count in the Smart Map Browser&apos;s list of publishers. (This setting applies to multi-publisher orgs only.)</description>
        <externalId>false</externalId>
        <inlineHelpText>Disable the solution count in the Smart Map Browser&apos;s list of publishers. (This setting applies to multi-publisher orgs only.)</inlineHelpText>
        <label>Disable Solutions Count</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DMAPP__Enable_New_Insight_Maps__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>This field is used to control whether users will see the new or classic insight map screens. Note Before enabling this setting, your administrator must have run the upgrade scripts to convert existing maps from the old to new formats. (Deprecated)</description>
        <externalId>false</externalId>
        <inlineHelpText>This field is used to control whether users will see the new or classic insight map screens. Note Before enabling this setting, your administrator must have run the upgrade scripts to convert existing maps from the old to new formats. (Deprecated)</inlineHelpText>
        <label>Enable New Insight Maps (Deprecated)</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DMAPP__Export_Version__c</fullName>
        <deprecated>false</deprecated>
        <description>The target version for which objects are to be export. This determines which fields will be exported for the objects</description>
        <externalId>false</externalId>
        <inlineHelpText>The target version for which objects are to be export, i.e. 8.26</inlineHelpText>
        <label>Export Version</label>
        <length>16</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>DMAPP__Licensed__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>This field is deprecated. Access to Dealmaker Align is controlled by license allocation.</description>
        <externalId>false</externalId>
        <inlineHelpText>This field is deprecated. Access to Dealmaker Align is controlled by license allocation.</inlineHelpText>
        <label>Licensed (Deprecated)</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
