    <validationRules>
        <fullName>Ineligible_Interconnection_Costs</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISPICKVAL( Status__c , &quot;New&quot;), ISNULL( Ineligible_Interconnection_EPC_watt__c ))</errorConditionFormula>
        <errorDisplayField>Ineligible_Interconnection_EPC_watt__c</errorDisplayField>
        <errorMessage>Enter the Ineligible Interconnection EPC $/watt costs</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Finance_can_edit_the_PPA_Terms</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  
 $User.UserRoleId  &lt;&gt; &apos;2F00E340000021DaD&apos;, 
OR(ISCHANGED( Rating__c), ISCHANGED( PPA_Annual_Escalation_Rate__c ),ISCHANGED( Rejection_Notes__c ), ISCHANGED( PPA_Price__c)))</errorConditionFormula>
        <errorMessage>Only the Finance Team can edit the PPA Price and Rating</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_for_No_Rating</fullName>
        <active>false</active>
        <description>Reason for No Rating must be filled in if Rating Agency is &quot;none&quot;</description>
        <errorConditionFormula>AND(ISPICKVAL(Credit_Rating_Agency__c, &apos;None&apos;),  Reason_for_No_Rating__c =&apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Reason_for_No_Rating__c</errorDisplayField>
        <errorMessage>Please give a reason for no credit rating.</errorMessage>
    </validationRules>
