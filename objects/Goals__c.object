    <validationRules>
        <fullName>Locks_Down_Manager_Comments</fullName>
        <active>false</active>
        <description>no one can edit Manager Comments accept Manager listed on EDT</description>
        <errorConditionFormula>AND(
ISCHANGED (Manager_Comments__c),
  $User.Id  &lt;&gt;  EmployeeDevelopmentTracker__r.Manager__r.Id 
)</errorConditionFormula>
        <errorDisplayField>Manager_Comments__c</errorDisplayField>
        <errorMessage>Only the manager listed on the Employee Development Tracker can edit this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Editing_Goals_once_approved</fullName>
        <active>true</active>
        <description>if Goals Approved is checked off - Goal Name, Goal Description and How to Measure cannot be edited.</description>
        <errorConditionFormula>AND(
 GoalsApproved__c  = TRUE,
OR(
ISCHANGED( DescriptionofGoal__c),
ISCHANGED( How_to_Measure_Success__c ),
ISCHANGED( Name )
)
)</errorConditionFormula>
        <errorMessage>Once Goals are approved you cannot edit Goal Name, Goal Description or How Goals are Measured.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Initiative_Required</fullName>
        <active>true</active>
        <description>a value must be placed in the Other Initiative field if Other Initiative is selected in the Annual Strategic Initiatives field.</description>
        <errorConditionFormula>AND(
INCLUDES( BorregoStrategicObjectives__c ,  &apos;Other initiative&apos;),
ISBLANK( TEXT(OtherInitiatives__c))
)</errorConditionFormula>
        <errorDisplayField>OtherInitiatives__c</errorDisplayField>
        <errorMessage>If the Annual Strategic Initiative includes the value of Other Initiatives, please provide additional information by selecting a value from the Other Initiatives picklist.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Strategic_Goals_Requires_Borrego_Strat</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK(BorregoStrategicObjectives__c ),
ISPICKVAL( Goal_Type__c , &quot;Strategic&quot;)
)</errorConditionFormula>
        <errorDisplayField>BorregoStrategicObjectives__c</errorDisplayField>
        <errorMessage>If you are creating a Strategic Goal you must align the goal with a Borrego Strategic Goal</errorMessage>
    </validationRules>
