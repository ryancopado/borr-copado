    <fields>
        <fullName>Warranty_Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF( AND((TODAY()- Purchase_Date__c &lt;1095),(ISPICKVAL( Product_Type__c, &quot;Laptop&quot;))), &quot;Under Warranty&quot;,
IF( AND((TODAY()- Purchase_Date__c &lt;1095),(ISPICKVAL( Product_Type__c, &quot;Desktop&quot;))), &quot;Under Warranty&quot;,
IF( AND((TODAY()- Purchase_Date__c &gt;1095),(ISPICKVAL( Product_Type__c, &quot;Laptop&quot;))), &quot;Warranty Expired&quot;,
IF( AND((TODAY()- Purchase_Date__c &gt;1095),(ISPICKVAL( Product_Type__c, &quot;Laptop&quot;))), &quot;Warranty Expired&quot;,
 &quot;N/A&quot;))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>If purchased less than 3 yrs ago, under warranty. If purchased more than 3 yrs ago, expired.</inlineHelpText>
        <label>Warranty Status</label>
        <required>false</required>
