    <webLinks>
        <fullName>DeleteInvoice</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>DeleteInvoice</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/16.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/16.0/apex.js&quot;)}

var INV_ID = &quot;{!Invoices__c.Id}&quot;;

var r=confirm(&quot;Are you sure?&quot;);
if (r == true)  {
    var result1 = sforce.apex.execute(&quot;deleteInvButton&quot; , &quot;deleteInvoice&quot;, {invID : INV_ID});
    if (result1 == &apos;null&apos;)  {
        alert(&quot;You cannot delete an invoice that has entered or completed the approval process.&quot;);
    } else {
        alert(&quot;Invoice deleted.&quot;);
    }
}</url>
    </webLinks>
