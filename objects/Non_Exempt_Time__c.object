        <fullName>OT_Hours_for_Week__c</fullName>
        <description>If CA Employee, sums all hours per day worked over 8 hours for each day. If not CA Employee, sums hours for week over 40.</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Employee_Name__r.Region__c, &quot;Northern CA&quot;) || 
  (ISPICKVAL(Employee_Name__r.Region__c, &quot;Southern CA&quot;)),

 Sunday_OT_Hours_Worked__c +
 Monday_OT_Hours_Worked__c +
 Tuesday_OT_Hours_Worked__c +
 Wednesday_OT_Hours_Worked__c +
 Thursday_OT_Hours_Worked__c +
 Friday_OT_Hours_Worked__c +
 Saturday_OT_Hours_Worked__c ,

IF(NOT(ISPICKVAL(Employee_Name__r.Region__c, &quot;Northern CA&quot;)) || 
   NOT(ISPICKVAL(Employee_Name__r.Region__c, &quot;Southern CA&quot;)) 

   &amp;&amp; Has_Overtime_been_reached__c = &quot;Yes&quot;,
   Total_Hours_for_Week__c - 40, 0))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>If CA Employee, sums all hours per day worked over 8 hours for each day. If not CA Employee, sums hours for week over 40.</inlineHelpText>
        <label>OT Hours for Week</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
