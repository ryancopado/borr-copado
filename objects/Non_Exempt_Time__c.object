    <fields>
        <fullName>Has_Overtime_been_reached__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(OR(ISPICKVAL(Employee_Name__r.Region__c, &quot;Northern CA&quot;),
ISPICKVAL(Employee_Name__r.Region__c, &quot;Southern CA&quot;)), 

OR(Monday_Total_Hours_Worked__c &gt; 8, 
Tuesday_Total_Hours_Worked__c &gt; 8, 
Wednesday_Total_Hours_Worked__c &gt; 8, 
Thursday_Total_Hours_Worked__c &gt; 8, 
Friday_Total_Hours_Worked__c &gt; 8, 
Saturday_Total_Hours_Worked__c &gt; 8, 
Sunday_Total_Hours_Worked__c &gt; 8,
Total_Hours_for_Week__c &gt; 40)), &quot;Yes&quot;, 

IF(AND(OR(NOT(ISPICKVAL(Employee_Name__r.Region__c, &quot;Northern CA&quot;)), 
NOT(ISPICKVAL(Employee_Name__r.Region__c, &quot;Southern CA&quot;))),

Total_Hours_for_Week__c &gt; 40), &quot;Yes&quot;, &quot;No&quot;))</formula>
        <inlineHelpText>looks to see if any days of the week include hours &gt; 8 if employee works in Southern or Northern CA, or total hours per week &gt; 40 for other regions</inlineHelpText>
        <label>Has Overtime been reached?</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
