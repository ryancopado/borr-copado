        <linkType>javascript</linkType>
        <masterLabel>Generate CSV</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var records = {!GETRECORDIDS( $ObjectType.PO_Invoice__c )};
 
if (records[0] == null){
alert(&quot;Please select at least one PO Invoice for CSV Generaton.&quot;)
} else
{
window.open(&quot;/apex/vfGeneratePOInvoiceCSV?id=&quot;+records);
}</url>
    </webLinks>
