    <validationRules>
        <fullName>Date_Pricing_Needed_by_Must_be_24_hours</fullName>
        <active>true</active>
        <description>Date_Pricing_Needed_by_Must_be_24_hours from Date Entered</description>
        <errorConditionFormula>Date_Pricing_Needed_By__c = Date_Request_Entered__c &amp;&amp; ISPICKVAL(Request_Status__c, &quot;New&quot;)</errorConditionFormula>
        <errorDisplayField>Date_Pricing_Needed_By__c</errorDisplayField>
        <errorMessage>Please allow at least 1 business day for this request to be processed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Includes_Energy_Storage</fullName>
        <active>true</active>
        <description>Forces ESD fields to be filled out if the Pricing Request includes energy storage and the request is submitted
EPC Record Type only</description>
        <errorConditionFormula>AND(
 RecordType.Name = &quot;EPC&quot;,
 Pricing_Request_Completed__c = True,
 Includes_Storage__c = TRUE,
 DATEVALUE(CreatedDate) &gt; DATE (2018,09,03),
 OR(
  ISBLANK(ESD_Final_Price__c ),
  ISBLANK( ESD_Direct_COGS__c )
  )
 )</errorConditionFormula>
        <errorMessage>If the Pricing Request includes energy storage, ESD Final Price and ESD Direct COGS must be filled out in the Response Section before marking Pricing Request Complete</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mobilization_Date_Required</fullName>
        <active>false</active>
        <description>Mobilization Date is required except on record creation. After record is created a workflow will bring in the Approximate Install Date from the Oppty. If Approx Install is blank then the user will have to manually enter a mobilization upon next edit.</description>
        <errorConditionFormula>AND(NOT(ISBLANK(CreatedDate)),
    ISBLANK(Mobilization_Date__c),
    DATEVALUE(CreatedDate) &gt;  DATE(2018,9,5) )</errorConditionFormula>
        <errorDisplayField>Mobilization_Date__c</errorDisplayField>
        <errorMessage>Mobilization Date is required (except on initial record creation).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PT_Required_upon_completion</fullName>
        <active>true</active>
        <description>requires process time field to be filled out when completed</description>
        <errorConditionFormula>AND( ISBLANK(Process_Time_hours__c),
CreatedDate &gt; DATETIMEVALUE( &quot;2016-02-25 00:00:00&quot; ),
Pricing_Request_Completed__c = TRUE)</errorConditionFormula>
        <errorDisplayField>Process_Time_hours__c</errorDisplayField>
        <errorMessage>Please enter estimated process time (in hours) that it took the AE to complete this Pricing Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Person_Responsible_Required_to_Submit</fullName>
        <active>true</active>
        <description>Person Responsible must be filled in for Pricing Request to be Submitted</description>
        <errorConditionFormula>AND(
 ISPICKVAL( Request_Status__c , &quot;Submitted&quot; ),
 ISBLANK( Person_Responsible__c )
)</errorConditionFormula>
        <errorMessage>Person Responsible must be filled out in order to submit a request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pricing_Request_Completed</fullName>
        <active>false</active>
        <description>ensures all fields are populated before completion box can be checked off.</description>
        <errorConditionFormula>AND(Pricing_Request_Completed__c = True, 

DATEVALUE(CreatedDate)&gt; DATE(2018,6,12),

OR((ISBLANK( Link_to_Pricing_Tool_on_box_com__c )),
(ISBLANK( Final_EPC_W__c  )),
(ISBLANK( Ineligible_costs__c)),
(ISBLANK( Final_EPC_Total__c  )),
(ISBLANK( Direct_Margin__c )),
(ISBLANK( Module_Price_Assumption__c )),
(ISBLANK( AE_Design_Notes_and_Scope_Concerns__c )),
(ISBLANK( System_Size_from_Pricing_Tool__c )),
(ISBLANK( Gross_Margin_from_the_Pricing_Tool__c ))))</errorConditionFormula>
        <errorDisplayField>Pricing_Request_Completed__c</errorDisplayField>
        <errorMessage>Pricing Request Completed cannot be checked off if any of the field within the Pricing Request Response section are blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Production_Guarantee_Level_Required</fullName>
        <active>true</active>
        <description>for any new pricing request created as of 12.01.15</description>
        <errorConditionFormula>ISBLANK( Production_Guarantee_Level__c ) &amp;&amp; ISNEW()</errorConditionFormula>
        <errorDisplayField>Production_Guarantee_Level__c</errorDisplayField>
        <errorMessage>Production Guarantee Level is required upon creation. If there is none, enter &quot;none&quot; in the field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Site_Visit_Checklist</fullName>
        <active>true</active>
        <description>Require site visit checklist to be filled out if &quot;used for PB3 Approval&quot; is Yes</description>
        <errorConditionFormula>AND(
ISPICKVAL( Used_for_PB3_Approval__c , &quot;Yes&quot;),
 Site_Visit_Checklist__c = null
)</errorConditionFormula>
        <errorDisplayField>Site_Visit_Checklist__c</errorDisplayField>
        <errorMessage>If this pricing request is uses for the PB3 Approval Process a Site Visit Checklist is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Union_Field_Has_a_Value</fullName>
        <active>true</active>
        <description>ensures the union field which is brought over from the opportunity has a value before submitting for approval</description>
        <errorConditionFormula>AND(
Opportunity__r.RecordTypeId = &quot;0123C000000GkAJQA0&quot;,
