        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/36.0/connection.js&quot;)}

var recipients = &quot;&quot;;
var emailUserId = &quot;{!Request_for_Information_RFI__c.Email_User_1Id__c}&quot;;
if (emailUserId != null &amp;&amp; emailUserId != &quot;&quot;) {
  recipients += &quot;{!Request_for_Information_RFI__c.Email_User_1__c}&quot;;
}
emailUserId = &quot;{!Request_for_Information_RFI__c.Email_User_2Id__c}&quot;;
if (emailUserId != null &amp;&amp; emailUserId != &quot;&quot;) {
  recipients += &quot;, &quot; + &quot;{!Request_for_Information_RFI__c.Email_User_2__c}&quot;;
}
emailUserId = &quot;{!Request_for_Information_RFI__c.Email_User_3Id__c}&quot;;
if (emailUserId != null &amp;&amp; emailUserId != &quot;&quot;) {
  recipients += &quot;, &quot; + &quot;{!Request_for_Information_RFI__c.Email_User_3__c}&quot;;
}
emailUserId = &quot;{!Request_for_Information_RFI__c.Email_User_4Id__c}&quot;;
if (emailUserId != null &amp;&amp; emailUserId != &quot;&quot;) {
  recipients += &quot;, &quot; + &quot;{!Request_for_Information_RFI__c.Email_User_4__c}&quot;;
}
emailUserId = &quot;{!Request_for_Information_RFI__c.Email_User_5Id__c}&quot;;
if (emailUserId != null &amp;&amp; emailUserId != &quot;&quot;) {
  recipients += &quot;, &quot; + &quot;{!Request_for_Information_RFI__c.Email_User_5__c}&quot;;
}
emailUserId = &quot;{!Request_for_Information_RFI__c.Email_User_6Id__c}&quot;;
if (emailUserId != null &amp;&amp; emailUserId != &quot;&quot;) {
  recipients += &quot;, &quot; + &quot;{!Request_for_Information_RFI__c.Email_User_6__c}&quot;;
}
emailUserId = &quot;{!Request_for_Information_RFI__c.Email_User_7Id__c}&quot;;
if (emailUserId != null &amp;&amp; emailUserId != &quot;&quot;) {
  recipients += &quot;, &quot; + &quot;{!Request_for_Information_RFI__c.Email_User_7__c}&quot;;
}
emailUserId = &quot;{!Request_for_Information_RFI__c.Email_User_8Id__c}&quot;;
if (emailUserId != null &amp;&amp; emailUserId != &quot;&quot;) {
  recipients += &quot;, &quot; + &quot;{!Request_for_Information_RFI__c.Email_User_8__c}&quot;;
}

var issueTracker = new sforce.SObject(&quot;Request_for_Information_RFI__c&quot;);

var r = confirm(&apos;Send Updated Response to the following users: &apos; + recipients + &apos;?&apos;);
if (r == true) {
  issueTracker.Id = &quot;{!Request_for_Information_RFI__c.Id}&quot;;
  issueTracker.Send_Updated_Response__c = true;
  
  var result = sforce.connection.update([issueTracker]); 
  
  if (result[0].getBoolean(&quot;success&quot;)) {
    alert(&quot;Latest Response sent.&quot;);
    window.parent.location.href=&quot;/{!Request_for_Information_RFI__c.Id}&quot;;
