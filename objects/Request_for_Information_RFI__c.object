    <validationRules>
        <fullName>Requested_Response_Greater_Than_24_Hours</fullName>
        <active>true</active>
        <description>Do not allow the requested response date to be set &lt;= TODAY()</description>
        <errorConditionFormula>AND(ISPICKVAL( Status__c,&quot;New&quot;), 
Request_Response_Date__c &lt;= TODAY(),
ISCHANGED(Request_Response_Date__c))</errorConditionFormula>
        <errorDisplayField>Request_Response_Date__c</errorDisplayField>
        <errorMessage>The requested Response Date has to be at least 1 day from today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Response_To_Complete</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(Response__c),OR(ISPICKVAL( Status__c ,&quot;Closed&quot;),ISPICKVAL( Status__c ,&quot;Resolved&quot;))&amp;&amp; RecordTypeId &lt;&gt; &quot;012800000007huT&quot;)</errorConditionFormula>
        <errorDisplayField>Response__c</errorDisplayField>
        <errorMessage>You are required to add a response before you can Resolve this issue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Response_Stage_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$User.ProfileId &lt;&gt; &quot;00e340000015RaK&quot;,
RecordTypeId = &quot;01280000000U9s1&quot;,
CreatedBy.ProfileId = &quot;00e340000015RaK&quot;,
ISPICKVAL( Response_Stage__c, &quot;&quot; )  )</errorConditionFormula>
        <errorDisplayField>Response_Stage__c</errorDisplayField>
        <errorMessage>You must fill in the Response Stage</errorMessage>
    </validationRules>
