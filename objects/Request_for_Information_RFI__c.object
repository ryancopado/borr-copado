    </validationRules>
    <validationRules>
        <fullName>Recommended_Solution_Read_Only</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT( ISNEW() ), 
CreatedBy.ProfileId = &quot;00e4C000000YMVM&quot;, 
PRIORVALUE( Recommended_Solution__c ) &lt;&gt; &quot;&quot;, 
ISCHANGED( Recommended_Solution__c ) 
)</errorConditionFormula>
