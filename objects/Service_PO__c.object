    <validationRules>
        <fullName>Anita_Lynne_Mary_only_change_vendor_date</fullName>
        <active>true</active>
        <description>Only Lynne, Mary, and Anita can change the vendor delivery date</description>
        <errorConditionFormula>AND( (ISCHANGED( SCR_Expected_Delivery_Date__c )), $User.LastName &lt;&gt; &quot;Parker&quot;, $User.LastName &lt;&gt; &quot;East&quot;, $User.LastName &lt;&gt; &quot;Orozco&quot; )</errorConditionFormula>
        <errorDisplayField>SCR_Expected_Delivery_Date__c</errorDisplayField>
        <errorMessage>Only Lynne, Mary, or Anita can update the Vendor Confirmed ship date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_Aaron_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND( $User.FirstName &lt;&gt; &quot;Aaron&quot;, ISPICKVAL(SCR_Status__c, &quot;Approved by Aaron&quot;))</errorConditionFormula>
        <errorDisplayField>SCR_Status__c</errorDisplayField>
        <errorMessage>Only Aaron Hall can change the Status to Approved by Aaron.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_Force_Rejection_Notes</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( SCR_Status__c, &apos;Rejected&apos;),  SCR_Accounting_Rejection_Notes__c = &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>SCR_Accounting_Rejection_Notes__c</errorDisplayField>
        <errorMessage>Please enter Rejection Notes so the PM will know what to fix.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_Product_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW() = FALSE, ISPICKVAL(SCR_Status__c , &quot;PM Approved&quot;),  SCR_Cost_Code_Cat__c = &quot;02-1215 M&quot;,  SCR_Product__c =&apos;&apos;)</errorConditionFormula>
        <errorDisplayField>SCR_Product__c</errorDisplayField>
        <errorMessage>Please enter the racking product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_Third_Party_Shipping</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW() = FALSE, ISPICKVAL(SCR_Third_Party_Shipping__c , &quot;Yes&quot;), SCR_Freight_Company__c = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>SCR_Freight_Company__c</errorDisplayField>
        <errorMessage>You must enter a Freight Company when Third Party Shipping equals Yes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCR_Vendor_Contact</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW() = FALSE, ISPICKVAL(SCR_Status__c , &quot;PM Approved&quot;),  SCR_Vendor_Contact__r.Email = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>SCR_Vendor_Contact__c</errorDisplayField>
        <errorMessage>Vendor Contact Email is required. Please go the the Vendor Contact page and enter the email address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tax_Exemption</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW() = FALSE, SCR_Tax_Exemption__c &lt;&gt; &apos;Exempt&apos; , SCR_Tax_Exemption__c &lt;&gt; &apos;Non-Exempt&apos; , SCR_Tax_Exemption__c &lt;&gt; &apos;Not Entered&apos;, SCR_Tax_Exemption__c &lt;&gt; &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>SCR_Tax_Exemption__c</errorDisplayField>
        <errorMessage>You may only enter Exempt or Non-Exempt as the Tax Exemption Status.</errorMessage>
    </validationRules>
