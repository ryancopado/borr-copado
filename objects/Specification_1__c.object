    <validationRules>
        <fullName>Lock_Record</fullName>
        <active>true</active>
        <errorConditionFormula>AND($User.ProfileId&lt;&gt; &quot;00e30000000iBpb&quot;, $User.ProfileId &lt;&gt;&apos;00e80000001OE5l&apos;, CreatedById &lt;&gt; $User.Id,Locked__c = true,OR(ISCHANGED(Requested_Response_Date__c), ISCHANGED(Date_Sent__c ), ISCHANGED(Contact__c), ISCHANGED(Locked__c) = FALSE))</errorConditionFormula>
        <errorMessage>Only Ryan Burrowbridge, Bryan Morrison or the record creator can unlock this record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requested_Response_Greater_Than_24_Hours</fullName>
        <active>false</active>
        <description>Do not allow the requested response date to be set &lt;= TODAY(). Made inactive as of Sprint 2018.06 c</description>
        <errorConditionFormula>AND(ISPICKVAL( Status__c,&quot;New&quot;), 
Requested_Response_Date__c &lt;= TODAY(),
ISCHANGED(Requested_Response_Date__c)
)</errorConditionFormula>
        <errorDisplayField>Requested_Response_Date__c</errorDisplayField>
        <errorMessage>The requested Response Date has to be at least 1 day from today.</errorMessage>
    </validationRules>
