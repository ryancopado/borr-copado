        <fullName>Re_assign</fullName>
        <availability>online</availability>
        <description>Push this button to re-assign the Submittal to another PE</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Re-assign</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/connection.js&quot;)} 

try { 
var newRecords = []; 
var c = new sforce.SObject(&quot;Submittal_1__c&quot;); 
c.id =&quot;{!Submittal_1__c.Id}&quot;; 
c.Status__c = &quot;Re-assigned&quot;;
newRecords.push(c); 
result = sforce.connection.update(newRecords); 

if( result[0].getBoolean( &quot;success&quot; ) ) { 
window.location.reload(); 
} 
else { 
alert( &quot;An error has occurred. Error:&quot; + result ); 
} 
} 
catch( e ) { 
alert( &quot;An unexpected error has occurred. Error:&quot; + e ); 
}</url>
