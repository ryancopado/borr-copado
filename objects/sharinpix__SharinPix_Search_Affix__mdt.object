<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Prefixes and suffixes to add to Salesforce IDs when performing Search using the SharinPix Search form.</description>
    <fields>
        <fullName>sharinpix__Affix_Type__c</fullName>
        <deprecated>false</deprecated>
