    <script>
        jQuery.noConflict();
        var needDomain;
        (function($) {
            vex.defaultOptions.className = 'vex-theme-default';

            $(document).ready(function() {
                $('.erp_required').click(function(e) {
                    vex.open({
                        content: '<div style="margin:25px;font-size:16px">Use of this feature requires the installation of the Accounting Seed Orders &amp; Inventory product. Please contact our Sales Team at <a href="mailto:info@accountingseed.com?Subject=ERP%20Product%20Info&Body=Please%20provide%20{!$User.FirstName}%20{!$User.LastName}%20at%20{!$Organization.Name}%20more%20info%20on%20the%20ERP%20product." target="_top">info@accountingseed.com</a> for further information.</div>'
                    });
                });

                $('.wo_required').click(function(e) {
                    vex.open({
                        content: '<div style="margin:25px;font-size:16px">Use of this feature requires Field Service Lightning to be enabled and the installation of the Accounting Seed Field Service Lightning connector. <br/> Please contact our Sales Team at <a href="mailto:info@accountingseed.com?Subject=ERP%20Product%20Info&Body=Please%20provide%20{!$User.FirstName}%20{!$User.LastName}%20at%20{!$Organization.Name}%20more%20info%20on%20the%20ERP%20product." target="_top">info@accountingseed.com</a> for further information.</div>'
                    });
                });

                needDomain = function () {
                    vex.open({
                        content: '<div style="margin:25px;font-size:16px">Use of this feature requires the installation of My Domain. Please refer to this <br/> <a href="https://developer.salesforce.com/docs/atlas.en-us.identityImplGuide.meta/identityImplGuide/domain_name_overview.htm" target="_blank"><u>My Domain - Knowledge Article</u></a> for further information.</div>'
                    });
                }
            });
        })(jQuery)
    </script>

