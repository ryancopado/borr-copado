        </apex:tab>
        <apex:tab label="Manage Banks" reRender="fastlink2" disabled="{!isError}">
            <apex:form >
                <apex:outputPanel id="existedAccountsForm" layout="block">
                    <apex:pageBlock title="Manage Banks" rendered="{!bankAccountExist}" helpTitle="Help for this Page" helpUrl="https://accountingseed.zendesk.com/hc/en-us/sections/204345628-Bank-Direct-Connect">
                        <apex:pageMessages />
                        <apex:pageBlockButtons >
                            <apex:actionStatus id="saveStatus" onstart="AcctSeed.ASModule.disableButtons('Processing...')" />
                            <apex:commandButton value="Edit" status="saveStatus" action="{!updateIFrame}" reRender="existedAccountsForm, fastlink2" oncomplete="AcctSeed.ASModule.reloadIFrame();" />
                            <apex:commandButton value="Delete" status="saveStatus" action="{!deleteBankAccount}" reRender="existedAccountsForm, fastlink2" oncomplete="AcctSeed.ASModule.reloadIFrame();" />
                        </apex:pageBlockButtons>
                        <apex:pageBlockSection columns="1">
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="Select Bank" />
                                <apex:selectList value="{!selectedBankAccount}" size="1" style="width:300px;">
                                    <apex:selectOptions value="{!bankAccountsOptions}" />
                                </apex:selectList>
                            </apex:pageBlockSectionItem>
                        </apex:pageBlockSection>
                    </apex:pageBlock>
                </apex:outputPanel>
            </apex:form>
            <apex:pageBlock title="Link Banks" helpTitle="Help for this Page" helpUrl="https://accountingseed.zendesk.com/hc/en-us/sections/204345628-Bank-Direct-Connect">
                <apex:pageMessage summary="When you link a bank, it may take several minutes for the bank accounts to be available in the Map Bank Accounts tab." severity="info" strength="3" />
                <apex:outputPanel id="fastlink2">
                    <form id="verifyForm" target="verifyframe" action="https://usyirestmasternode.yodleeinteractive.com/authenticate/accountingseed/?channelAppName=usyirestmaster" method="post" style="width:100%;display:none">
                        <input type="text" name="app" value="10003600" />
                        <input type="text" name="rsession" value="{!UserSession}" />
                        <input type="text" name="token" value="{!AuthToken}" />
                        <input type="text" name="redirectReq" value="true" />
                        <input type="text" name="extraParams" value="{!extraParams}" />
                        <input type="submit" />
                    </form>
                </apex:outputPanel>
                <div id="iframeDiv"></div>
            </apex:pageBlock>
        </apex:tab>
        <apex:tab label="Map Bank Accounts" disabled="{!isError}" onTabEnter="refreshMappingTab();">
            <apex:form id="mappingForm">
                <apex:pageBlock title="Map Bank Accounts" id="mappingBlock" helpTitle="Help for this Page" helpUrl="https://accountingseed.zendesk.com/hc/en-us/sections/204345628-Bank-Direct-Connect">
                    <apex:pageMessages escape="false"/>
                    <apex:pageMessage summary="No bank accounts have been added. You must first add a bank account before you can map to a GL account.
" severity="info" strength="3" rendered="{!!bankSubAccountExist}" />
                    <apex:pageBlockButtons >
                        <apex:actionStatus id="saveStatus" onstart="AcctSeed.ASModule.disableButtons('Processing...')" />
                        <apex:actionStatus id="updateStatus"
                                           onstart="AcctSeed.ASModule.disableButtons('Processing...')"
                                           onStop="AcctSeed.ASModule.enableButtons()" />
                        <apex:commandButton id="saveButton"
                                            value="Save"
                                            action="{!saveMapping}"
                                            status="saveStatus"
                                            rendered="{!bankSubAccountExist}"
                                            reRender="mappingForm"/>
                        <apex:commandButton id="refreshButton"
                                            value="Refresh"
                                            status="saveStatus"
                                            rendered="{!bankSubAccountExist}"
                                            onComplete="refreshMappingTab();"  />
                    </apex:pageBlockButtons>
                    <apex:pageBlockSection columns="2" collapsible="false">
                        <apex:pageBlockSectionItem >
                            <apex:pageBlockTable value="{!bankSubAccounts}" var="bankSubAccount" rendered="{!bankSubAccountExist}">
                                <apex:column >
                                    <apex:facet name="header">Bank Account</apex:facet>
                                    {!bankSubAccount.bankSubAccountName}
                                </apex:column>
                                <apex:column >
                                    <apex:facet name="header">Ledger</apex:facet>
                                    <apex:outputText value="{!bankSubAccount.ledgerOptions[0].label}"
                                                     rendered="{!!bankSubAccount.showPicklist}" />
                                    <apex:selectList value="{!bankSubAccount.ledgerId}"
                                                     id="ledger"
                                                     multiSelect="false"
                                                     size="1" rendered="{!bankSubAccount.showPicklist}">
                                        <apex:selectOptions value="{!bankSubAccount.ledgerOptions}" />
                                    </apex:selectList>
                                </apex:column>
                                <apex:column >
                                    <apex:facet name="header">GL Account</apex:facet>
                                    <apex:selectList value="{!bankSubAccount.mappedTo}"
                                                     multiSelect="false"
                                                     size="1">
                                        <apex:selectOptions value="{!bankSubAccount.glAccountOptions}"/>
                                        <apex:actionSupport event="onchange"
                                                            action="{!changeGlAccount}"
                                                            rerender="ledger"
                                                            status="updateStatus"/>
                                    </apex:selectList>
                                </apex:column>
                            </apex:pageBlockTable>
                        </apex:pageBlockSectionItem>
                    </apex:pageBlockSection>
                </apex:pageBlock>
            </apex:form>
        </apex:tab>
    </apex:tabPanel>
</apex:page>