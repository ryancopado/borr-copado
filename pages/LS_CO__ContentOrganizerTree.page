        function f_filterResults(n_win, n_docel, n_body) {
            var n_result = n_win ? n_win : 0;
            if (n_docel && (!n_result || (n_result > n_docel)))
                n_result = n_docel;
            return n_body && (!n_result || (n_result > n_body)) ? n_body : n_result;
        }
     
     
     
     function f_scrollTop()
        {
            return f_filterResults (
                window.pageYOffset ? window.pageYOffset : 0,
                document.documentElement ? document.documentElement.scrollTop : 0,
                document.body ? document.body.scrollTop : 0
            );
        }

    function loadLoaderDialog(){
                overlayLoaderBlock();
                
                $('.loaderdialog').bind('load', function(){
                    $(this).parent().parent().offset({left: $('html').width()/2 - ($('.loaderdialog').width()/2 < 200 ? 200 : $(this).width()/2)});
                });
                
                $('.loaderdialog').css('top', '0px');
                $('.loaderdialog').css('left', '0px');
                $('.loaderdialog').fadeIn();
                $('.loaderdialog').offset({left: $('html').width()/2 - ($('.loaderdialog').width()/2 < 200 ? 200 : $('.loaderdialog').width()/2), top: f_scrollTop()+70});
   }
   
   function overlayLoaderRelease()
        {
            $('#loaderDialog').hide();
            $('.loaderdialog').css("display","none");
            $('.loaderMask-overlay').hide();
             window.location.reload();
        }

  </script>
