    <script>
        function downloadAllContent(size) {
            var i = 1;
            var myInterval = window.setInterval (
                function () {
                   if( i > size ) {
                       clearInterval(myInterval);
                       var downloadBut = document.getElementById('downloadall');
                       downloadBut.disabled  = true;
                       return;
                   }
                   var testt = document.getElementById('download'+i);
                   testt.onclick(); 
                   i++; 
                },
                5000 );
         }
         function downLoadContentOnClick(Id){         
             coLogs(Id, '{!$Label.LS_CO__Downloads}');
             window.location.href = '{!baseUrl}/sfc/servlet.shepherd/version/download/' + Id;
         }
    </script>
