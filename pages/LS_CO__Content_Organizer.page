    function masterCheckbox()
        {
            $('.row-checkbox').each(function(){
                if($('.master-checkbox').attr('checked'))
                    $(this).attr('checked', true);
                else
                    $(this).attr('checked', false);
                if($('.master-checkbox').attr('checked'))
                    $(this).parent().parent().addClass('selected');
                else
                    $(this).parent().parent().removeClass('selected');
            });
        }




 function recountCheckedContent()
        {
            selectedContentString = '';
            
            if($('.row-checkbox:checked').length == 0)
            {
                $('.actions-box input').attr('disabled', true);
                $('.actions-box input').removeClass('btn');
                $('.actions-box input').addClass('btnDisabled');
            }
            else
            {
                $('.row-checkbox:checked').each(function(){selectedContentString += $(this).attr('id')+',';});
