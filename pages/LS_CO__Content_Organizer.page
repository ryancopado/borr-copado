    var activeTab = "{!activeTab}";
    if(activeTab == '11'){
        $("#tabs div:first").attr("id","current"); // Activate first tab
        $("#content div:first").fadeIn(); // Show first tab content
    }else if(activeTab == '22'){       
        $("#tabs #22").attr("id","current"); // Activate first tab
        $('#tab2').fadeIn();
    }else if(activeTab == '33'){       
        $("#tabs #33").attr("id","current"); // Activate first tab
        $('#tab3').fadeIn();
    }else if(activeTab == '44'){ 
       try{  
            document.getElementById('84').setAttribute("id", "current");  
       } catch(err) {}
       try{     
            $("#tabs #84").attr("id","current"); // Activate first tab            
       } catch(err) {}
       $('#tab4').fadeIn();
    }else if(activeTab == '55'){ 
        try{
            document.getElementById('85').setAttribute("id", "current"); 
        } catch(err) {}
        try{    
            $("#tabs #85").attr("id","current"); // Activate first tab            
        } catch(err) {}
        $('#tab5').fadeIn();
    }else if(activeTab == '66'){ 
        try{ 
            document.getElementById('86').setAttribute("id", "current");  
        } catch(err) {}
        try{          
            $("#tabs #86").attr("id","current"); // Activate first tab            
        } catch(err) {}
        $('#tab6').fadeIn();
    }else if(activeTab == '77'){
        try{
           document.getElementById('87').setAttribute("id", "current"); 
        } catch(err) {}
        try{         
           $("#tabs #87").attr("id","current"); // Activate first tab            
        } catch(err) {}
        $('#tab7').fadeIn();
    }else if(activeTab == '88'){
        try{
           document.getElementById('88').setAttribute("id", "current");  
        } catch(err) {}
        try{        
           $("#tabs #88").attr("id","current"); // Activate first tab            
        } catch(err) {}
        $('#tab8').fadeIn();
    }
    
    
    
    $('#tabs a').click(function(e) {
        e.preventDefault();        
       $("#content .tab_content_main").hide(); //Hide all content
