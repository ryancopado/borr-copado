<script type="text/javascript">

function getInternetExplorerVersion() {

        var rv = -1; 
        if (navigator.appName == 'Microsoft Internet Explorer') {
            var ua = navigator.userAgent;
            var re = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
            if (re.exec(ua) != null)
                rv = parseFloat(RegExp.$1);
        }
        return rv;
    }
    function checkVersion() {
       
        var ver = getInternetExplorerVersion();
        if (ver > -1) {
            if (ver == 8.0)
            {
               
            
                
                document.getElementById("share_ie").className = "btm_btn_ie";
                document.getElementById("archive_ie").className = "btm_btn_ie";
                document.getElementById("applytag_ie").className = "btm_btn_ie";
                document.getElementById("removetag_ie").className = "btm_btn_ie";
                document.getElementById("download_ie").className = "btm_btn_ie";
                document.getElementById("delete_ie").className = "btm_btn_ie";
              
            }
         if (ver == 7.0 || ver == 8.0)
            {   
                
                document.getElementById("content").className = "content_style_ie8";
                document.getElementById("search_rslt").className = "search_results_ie7_8";
