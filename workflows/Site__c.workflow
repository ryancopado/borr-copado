        <actions>
            <name>Site_request_proposal_for_witness_test_reminder_to_EE_and_PM</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>emails EE and PM when As Built is reached as a design stage to remind them to request proposal for witness test</description>
        <formula>AND(
ISPICKVAL(  Design_Stage__c , &quot;As Built&quot; ),
NOT(BEGINS( Project__r.Job_Number__c , &quot;201&quot; )),
NOT(BEGINS( Project__r.Job_Number__c , &quot;801&quot; )),
NOT(BEGINS( Project__r.Job_Number__c , &quot;902&quot; )),
NOT(BEGINS( Project__r.Job_Number__c , &quot;901&quot; ))
)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
